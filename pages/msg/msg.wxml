<view class="msg-center">
  <view class="msg-center-items">
    <view  wx:for="{{msgList}}" wx:key="id" wx:for-item="item" class="msg-center-item">
      <view class='msg-center-item-head-title'>{{item.title}}</view>
      <view class='msg-center-item-head-content'>{{item.content}}</view>
      <view class="msg-center-item-foot">
        <text>{{item.creatTime}}</text>
      </view>
    </view>
  </view>
</view>