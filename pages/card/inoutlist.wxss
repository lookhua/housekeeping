page {
  background: #f4f3f3;
  padding: 0px;
}

.inout-center {
}

inout-center-items{
  
}

.inout-center-item {
  background: white;
  margin-bottom: 20rpx;
  margin-top: 20rpx;
  padding: 0rpx 40rpx;
}

.inout-center-item-head {
  position: relative;
}

.inout-center-item-head-title {
  padding: 16rpx 0rpx;
  color: #222222;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
}
.green{
  color: green
}
.red{
  color: red
}

.inout-center-item-head-content {
  font-size: 30rpx;
  right: 0rpx;
  word-break: break-all;
  color: #b5b5b5;
}

.inout-center-item-foot {
  font-size: 28rpx;
  text-align: right;
  color: #7e7e7e;
}
